<Window x:Class="NBApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NBApp"
        mc:Ignorable="d" ResizeMode="CanMinimize"
        Title="NBApp" Height="480" Width="800" Icon="/Resources/logo.jpg">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition Height="20"/>
        </Grid.RowDefinitions>
        <Frame x:Name="MainFrame" Focusable="False" NavigationUIVisibility="Hidden"/>
        <TextBlock x:Name="TbNBAyears" Grid.Row="1" HorizontalAlignment="Center" TextAlignment="Center" FontSize="16"/>
    </Grid>
</Window>
